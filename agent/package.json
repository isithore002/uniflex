{
  "name": "uniflux-agent",
  "version": "1.0.0",
  "description": "Autonomous Liquidity Maintenance Agent for Uniswap v4",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:loop": "ts-node src/index.ts --loop",
    "server": "tsx src/server.ts",
    "server:auto": "AUTO_MODE=true tsx src/server.ts",
    "server:auto:win": "set AUTO_MODE=true && tsx src/server.ts",
    "prod": "AUTO_MODE=true node --loader tsx src/server.ts",
    "test": "tsx src/test-e2e.ts",
    "test:e2e": "tsx src/test-e2e.ts"
  },
  "keywords": [
    "uniswap",
    "v4",
    "agent",
    "defi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@lifi/sdk": "^3.15.4",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "express": "^5.2.1",
    "tsx": "^4.21.0",
    "viem": "^2.45.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
